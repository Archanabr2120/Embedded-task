#include<stdio.h>
int main()
{
    int i,n,j,a=2,b=2;
    printf("Enter the number: ");
    scanf("%d",&n);
    for(i=1;i<=n;i++)
    {
        for(int k=1;k<i;k++){
            printf(" ");
            }
        for(j=1;j<=(n+1-i);j++)
        {
         printf("%d",i);  
         
        }
        printf("\n");
    }
    return 0;
}
