#include <stdio.h>
#include <string.h>

int main() {
    char str1[20], str2[20];
    int i = 0;

    printf("Enter String 1: ");
    scanf("%s", str1);

    printf("Enter String 2: ");
    scanf("%s", str2);

    while (str2[i] != '\0') {
        str1[i] = str2[i];
        i++;
    }
  
    str1[i] = '\0';

    printf("String 1: %s\n", str1);
    printf("String 2: %s\n", str2);

    return 0;
}

