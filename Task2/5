#include <stdio.h>

int main() {
    int a[100],i,n,b[100];
    printf("Enter the number of elements of array:");
    scanf("%d",&n);
    
    for(i=0;i<n;i++)
    {
        
        printf("Enter the number %d:",(i+1));
        scanf("%d",&a[i]);
        b[n-i-1]=a[i];
    }
    printf("The Inverted array elements are:\n");
    for(i=0;i<n;i++)
    {
        printf("%d\n",b[i]);
    }
    
    return 0;
}
