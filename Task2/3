#include <stdio.h>

int main() {
    int a[100],i,n,b[100],c[100],s,j;
    printf("Enter the number of elements of arrays:");
    scanf("%d",&n);
    
    for(i=0;i<n;i++)
    {
        
        printf("Enter the number %d  of array 1:",(i+1));
        scanf("%d",&a[i]);
        
    }
         
    for(i=0;i<n;i++)
    {
        printf("Enter the number %d of array 2 :",(i+1));
        scanf("%d",&b[i]);
    }
     
    for(i=0;i<n;i++)
    {
        s=a[i]+b[i];
        c[i]=s;
    }
    printf("\nSum of two arrays are:\n");
    for(j=0;j<n;j++)
    {
        printf("%d\n",c[j]);
    }
    return 0;
}
