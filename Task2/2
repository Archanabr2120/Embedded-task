#include <stdio.h>

int main() {
    int a[100],i,n,b[100],c,j;
    printf("Enter the number of elements of arrays:");
    scanf("%d",&n);
    for(i=0;i<n;i++)
    {
        
        printf("Enter the number %d :",i+1);
        
        scanf("%d",&a[i]);
        c=15+a[i];
        b[i]=c;
    }
    printf("\nSum of two arrays are:\n");
    for(j=0;j<n;j++)
    {
        printf("%d\n",b[j]);
    }
    return 0;
}
