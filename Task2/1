#include <stdio.h>

int main() {
    int a[100],i,n,b;
    int even=0,odd=0;
    printf("Enter the number of elements:");
    scanf("%d",&n);
    for(i=0;i<n;i++)
    {
        printf("Enter the number %d :",i+1);
        scanf("%d",&a[i]);
        b=a[i]%2;
        if(b==0)
        {
            even = even+a[i];
        }
        else
        {
            odd=odd+a[i];
        }
    }
    printf("\nSum of even numbers: %d",even);
    printf("\nSum of odd numbers: %d",odd);
    
    return 0;
}
