#include <stdio.h>
#include <string.h>

int main() {
    char str1[20], str2[20];
    int i = 0;

    printf("Enter String 1: ");
    scanf("%s", str1);

    printf("Enter String 2: ");
    scanf("%s", str2);

    int n1 = strlen(str1);
    int n2 = strlen(str2);
    int n = n1 + n2;

    for (i = 0; i < n2; i++) 
    {
        str1[n1 + i] = str2[i];
    }
    
    str1[n] = '\0'; 

    printf("Concatenated string: %s\n", str1);
    printf("String 2: %s\n", str2);

    return 0;
}

